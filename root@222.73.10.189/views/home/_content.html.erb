
<%i=1
for top_cate in @all_cates%>
  <div class="<%=(i==1 ? 'contentB':'contentB hidden')%>" id="c2_<%=i%>">
    <div class="contentB_a border">

      <%for sub_cate in top_cate.middle_products.limit(3)%>
        <dl>
          <dt><%=sub_cate.name%></dt>
          <dd>
            <%for third_cate in sub_cate.end_products%>
              <%=link_to third_cate.name,{:controller=>"products",:action=>"search",:search_key=>third_cate.name}%>
            <%end%>
          </dd>
        </dl>
      <%end%>

    </div>
    <div class="contentB_b border">
      <h1><%=top_cate.name%>热搜榜</h1>
      <ul>
        <li><a href="#">冷暖变频空调</a></li>
        <li><a href="#">全自动洗衣机</a></li>
        <li><a href="#">飞科电动剃须刀</a></li>
        <li><a href="#">37寸液晶电视</a></li>
        <li><a href="#">脱排油烟机</a></li>
        <li><a href="#">美的电饭煲</a></li>
        <li><a href="#">苏泊尔电压力锅</a></li>
      </ul>
    </div>
    <div class="contentB_c border">
      <h1><%=top_cate.name%>商家榜</h1>
      <ul>
        <%for merchant in top_cate.merchants.limit(12)%>
          <li><%=link_to merchant.name,merchant.merchanturl%></li>
        <%end%>
      </ul>
    </div>
    <div class="contentB_d ">
      <h1><%=top_cate.name%>品牌</h1>
      <ul>
        <%for brand in top_cate.brands.limit(12)%>
          <li><%=link_to image_tag(brand.brandlogo_url),brand.brandurl%></li>
        <%end%>
        <%if top_cate.brands.size > 12%>
          <li><%=link_to ">>>更多","#"%></li>
        <%end%>
      </ul>
    </div>
  </div>
  <%i=i+1
end%>
