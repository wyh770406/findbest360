<%#= spaceless do %>
<%if params[:from]=="merchant"%>
  <div class="tools">
    <a href="<%= adminpanel_merchant_products_path(@merchant_id) %>" class="button small left<%= ' checked' if params[:action] == "index" %>">查看所有</a>
    <% if ["edit","update"].index(params[:action]) %>
      <a href="#" class="button small checked">编辑</a>
    <% end %>
    <a href="<%= new_adminpanel_merchant_product_path(@merchant_id,:from=>"merchant") %>" class="button small right<%= ' checked' if ["new","create"].index(params[:action]) %>">新建</a>
  </div>
<%elsif params[:from]=="end_product"%>
  <div class="tools">
    <a href="<%= adminpanel_top_product_middle_product_end_product_products_path(@top_product,@middle_product,@end_product) %>" class="button small left<%= ' checked' if params[:action] == "index" %>">查看所有</a>
    <% if ["edit","update"].index(params[:action]) %>
      <a href="#" class="button small checked">编辑</a>
    <% end %>
    <a href="<%= new_adminpanel_top_product_middle_product_end_product_product_path(@top_product,@middle_product,@end_product,:from=>"end_product") %>" class="button small right<%= ' checked' if ["new","create"].index(params[:action]) %>">新建</a>
  </div>
<%end%>
<%# end %>
