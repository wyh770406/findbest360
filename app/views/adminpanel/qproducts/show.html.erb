<%#= render 'base' %>
<div class="tools">
  <%if params[:from]=="end_product"%>
    <%= link_to '修改', edit_adminpanel_top_product_middle_product_end_product_product_path(@top_product,@middle_product,@end_product,@product) %> |
    <%= link_to '返回', adminpanel_top_product_middle_product_end_product_products_path(@top_product,@middle_product,@end_product) %>
  <%elsif params[:from]=="merchant"%>
    <%= link_to '修改', edit_adminpanel_merchant_product_path(@merchant,@product) %> |
    <%= link_to '返回', adminpanel_merchant_products_path(@merchant,@product) %>
  <%end%>
</div>
<div id="adminpanel_product">
  <p>
    <b>Name:</b>
    <%= @product.title %>
  </p>

</div>
