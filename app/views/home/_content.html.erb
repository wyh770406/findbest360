
<%i=1
for top_cate in @all_cates%>
  <div class="<%=(i==1 ? 'contentB':'contentB hidden')%>" id="c2_<%=i%>">
    <div class="contentB_a border">

      <%for sub_cate in top_cate.middle_products.asc("order_num").limit(9)%>
        <dl>
          <dt><%=sub_cate.name[0,4]%></dt>
          <dd>
            <%sub_cate_end_products=sub_cate.end_products.asc("order_num")
              for third_cate in sub_cate_end_products[0,6]%>
              <%=link_to third_cate.name,end_product_products_path(third_cate),:target=>"_blank"%>
              <%#=link_to third_cate.name,{:controller=>"products",:action=>"search",:search_key=>third_cate.name},:target=>"_blank"%>
            <%end%>
            <%if sub_cate_end_products.uniq.size>6%><span><a href="#" class="anchorclass myownclass" rel="submenu3">>></a></span>
<div id="submenu3" class="anylinkcsscols">
<%
if !sub_cate_end_products.uniq[6,6].nil?%>
<div class="column">
 <b>[<%=sub_cate.name%>]</b>
 <ul>
            <%for third_cate in sub_cate_end_products.uniq[6,6]%>
              <li><%=link_to third_cate.name,end_product_products_path(third_cate),:target=>"_blank"%></li>
              <%#=link_to third_cate.name,{:controller=>"products",:action=>"search",:search_key=>third_cate.name},:target=>"_blank"%>
            <%end%>
 </ul>
</div>
<%
end%>
<%
if !sub_cate_end_products.uniq[12,6].nil?%>
<div class="column">
 <b></b>
 <ul>
            <%for third_cate in sub_cate_end_products.uniq[12,6]%>
              <li><%=link_to third_cate.name,end_product_products_path(third_cate),:target=>"_blank"%></li>
              <%#=link_to third_cate.name,{:controller=>"products",:action=>"search",:search_key=>third_cate.name},:target=>"_blank"%>
            <%end%>
 </ul>
</div>
<%
end%>
<%
if !sub_cate_end_products.uniq[18,6].nil?%>
<div class="column">
 <b></b>
 <ul>
            <%for third_cate in sub_cate_end_products.uniq[18,6]%>
              <li><%=link_to third_cate.name,end_product_products_path(third_cate),:target=>"_blank"%></li>
