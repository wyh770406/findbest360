        <%for merchant in top_cate.merchants.asc("order_num").limit(9)%>
          <li><%=link_to merchant.name,merchant.merchanturl,:target=>"_blank"%></li>
        <%end%>
      </ul>
    </div>
    <div class="contentB_d ">
      <h1><%if ["电脑手机数码","旅游酒店机票","食品/保健品","家电/办公"].include?(top_cate.name)%>
         <%=top_cate.name[0,6]%>
        <%else%>
         <%=top_cate.name[0,6]%>品牌
         <%end%></h1>
      <ul>
        <%for brand in top_cate.brands.asc("order_num").limit(5)%>
          <li><%=link_to image_tag(brand.brandlogo_url),brand.brandurl,:target=>"_blank"%></li>
        <%end%>
        <%if top_cate.brands.size > 5%>
          <li><%= link_to ">>>更多", brands_path(:product_id => top_cate.id),:target=>"_blank" %></li>
        <%end%>
      </ul>
    </div>
  </div>
  <%i=i+1
end%>
